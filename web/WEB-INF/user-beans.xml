<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation=" http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context  http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:component-scan base-package="insat" />
    <context:annotation-config />

    <bean id="userService" class="insat.service.UserServiceImpl" />
    <bean id="userDao" class="insat.dao.UserDaoImpl" />
    <bean id="postService" class="insat.service.PostServiceImpl" />
    <bean id="postDao" class="insat.dao.PostDaoImpl" />
    <bean id="adminService" class="insat.service.AdminServiceImpl" />
    <bean id="adminDao" class="insat.dao.AdminDaoImpl" />
    <bean id="rateService" class="insat.service.RateServiceImpl" />
    <bean id="rateDao" class="insat.dao.RateDaoImpl" />

</beans>